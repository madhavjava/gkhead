<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
       <class name="org.mifos.platform.questionnaire.domain.ChoiceTagEntity" table="question_choice_tags">
           <id name="id" column="ID" type="integer" unsaved-value="0" access="field">
               <generator class="native"/>
           </id>
           <property name="tagText" column="tag_text" not-null="true"/>
       </class>
</hibernate-mapping>